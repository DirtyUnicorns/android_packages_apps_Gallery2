<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- String indicating how many media items from the camera have been selected -->
    <plurals name="ingest_number_of_items_selected">
        <item quantity="zero">%1$d élément sélectionné</item>
        <item quantity="one">%1$d élément sélectionné</item>
        <item quantity="other">%1$d éléments sélectionnés</item>
    </plurals>

    <!-- A label describing to link website [CHAR LIMIT=30] -->
    <string name="more_image">Plus d\'images</string>
    <string name="website_for_more_image"></string>

    <!-- Toast if the muted video is not supported. [CHAR LIMIT=80] -->
    <string name="mute_nosupport">Impossible de couper le son : vidéo non prise en charge</string>

    <!-- The message is shown in toast when click showOnMap Menu and there is no map app -->
    <string name="map_activity_not_found_err">Aucune application de localisation installée pour afficher l\'emplacement</string>

    <!-- The label for the bass boost knob of the audio effects dialog. -->
    <string name="bass_boost_strength">Amplification des basses</string>

    <!-- The label for the 3d effect knob of the audio effects dialog. -->
    <string name="virtualizer_strength">Effet 3D</string>

    <!-- The label for the audio effects menu. -->
    <string name="audio_effects">Effets audio</string>

    <!-- Toast if user attempts to control audio effects without headphones plugged in. -->
    <string name="headset_plug">Brancher un casque pour utiliser ces effets</string>

    <!-- The title of the audio effects dialog. -->
    <string name="audio_effects_dialog_title">Snapdragon Audio+</string>

    <!-- The tips of trimming video -->
    <string name="fail_trim">Découpe de la vidéo impossible</string>

    <!-- Group by Tags tab on Action Bar. [CHAR LIMIT=12] -->
    <string name="tags">Étiquettes</string>

    <!-- Title of a menu item to group photo by tags [CHAR LIMIT=30]-->
    <string name="group_by_tags">Par étiquette</string>
    <!-- Message for first run dialog asking if the user wants to remember photo locations [CHAR LIMIT = None] -->
    <string name="remember_location_prompt">Ajoutez des étiquette à vos photos et à vos vidéos pour identifier l\'endroit de la prise de vue.\n\nD\'autres applications peuvent accéder à ces informations, ainsi qu\'aux images enregistrées.</string>
</resources>
